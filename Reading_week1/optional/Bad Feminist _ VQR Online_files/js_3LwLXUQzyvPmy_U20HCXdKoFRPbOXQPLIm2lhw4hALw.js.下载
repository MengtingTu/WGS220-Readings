// base function for using flexslider
(function ($, Drupal) {
  // All your code here


	// Handle hash changes
	function locationHashChanged () {
		switch(window.location.hash) {
			// Hide/Show Comments
			case '#comments': 
				$('#comments').addClass('is-open');
			break;
		}
	}

	$(document).ready(function() {
		// Add toggle class to expandable nav items
		$(".responsive-container").fitVids({customSelector: "iframe[src^='http://in.fm']"});
		$(".node-story.node-teaser .field-embed-code").fitVids({customSelector: "iframe[src^='http://in.fm']"});

		$('.menu-level-1 > .menu > li.expanded > a').click(function (e) {
			e.preventDefault();
			$(this).parent('li').toggleClass('menu-open');
			$(this).parent('li').siblings('li').removeClass('menu-open');
		});

		// Hide/Show Search
		$('#toggle-search').click(function (e) {
			e.preventDefault();
			$(this).toggleClass('is-open');
			$('#search-block-form').toggleClass('is-open');
		});

		// Remove #backtotop if below the bottom of the story's body
		if ($('#backtotop').length && $('.node-type-story').length) {
			$(window).bind('scroll', function () {
				var scroll_bottom = $(this).height() + $(this).scrollTop();
				var field_body_height = $('.node-type-story .field-body').innerHeight() + $('.node-type-story .field-body').offset().top;
				var bottom = 0;
				if (scroll_bottom > field_body_height) {
					bottom = scroll_bottom - field_body_height;
					$('#backtotop').css('visibility', 'hidden');
				}
				else {
					$('#backtotop').css('visibility', 'visible');
				}
			});
		}

		// Toggle font sizes 
		$('.font-sizes [data-font-size]').click(function (e) {
			e.preventDefault();
			$('.field-body').removeClass('text-small text-medium text-large').addClass($(this).attr('data-font-size'));
			$('.font-sizes [data-font-size]').removeClass('active');
			$(this).addClass('active');
		});

		// Print link (since Drupal strips out :javascript in href)
		$('a#print').click(function (e) {
			e.preventDefault();
			window.print();
		});


		// Attatch the caption to the colorbox object, and output the formatted caption
		$('[class*="media-colorbox"]').each(function (i, el) {
			$(this).find('.cboxElement').data('caption', $(this).find('.field-cap').html());
		});
		$(document).bind('cbox_complete', function() {
		    $('#cboxTitle').html($.colorbox.element().data('caption'));
		});

		// Subscription page - submit the related hidden "add to cart" form for subscriptions
		$('.price-table').each(function () {

			// Submit appropriate "add to cart"
			$(this).find('.button').click(function (e) {
				e.preventDefault();
				var pid = $('input[name="subscription"]:checked').data('pid');
				if (typeof pid !== 'undefined') {
					$('form.commerce-cart-add-to-cart-form-'+pid).submit();
				}
				
			});

			// Highlight checked radio buttons
			$(this).find('input[type="radio"]').change(function () {
				$('input[type="radio"]').parents('td').css('background-color', 'transparent');
				$('input[type="radio"]:checked').parents('td').css('background-color', '#f2d265');
			});

			// Make table cell clickable
			$(this).find('td').click(function() {

				$(this).find('input[name="subscription"]').prop('checked', true);
				$('input[type="radio"]').parents('td').css('background-color', 'transparent');
				$('input[type="radio"]:checked').parents('td').css('background-color', '#f2d265');
			}).css('cursor', 'pointer');;

		});
		
		// Listen for hash changes
		window.onhashchange = locationHashChanged;
		locationHashChanged();

		// Hook into foundation
		$(document).foundation();

	});
	Drupal.behaviors.something = {
		attach: function (context, settings) {
			/*
			$('.toggle-topbar a').click(function(e) {
				e.preventDefault();
				$('.top-bar').toggleClass('expanded');

			});
			$('.top-bar.expanded .has-dropdown a').click(function(e) {
				e.preventDefault();
				$(this).parent().addClass('moved');

			});
			$('.top-bar.expanded .back').click(function(e) {
				e.preventDefault();
				$(this).parent().parent().removeClass('moved');

			});
			*/

			$('#nav-toggle, #nav-toggle-close').click(function(e) {
				e.preventDefault();
				$('.main-menu').toggleClass('open');
				$('body').toggleClass('nav-open');
			});
			$(window).resize(function() {
				// this is here to demonstrate tapping into media queries in JS using Modernizr
				$('body').removeClass('mq-phone');
				$('body').removeClass('mq-phone_wide-sm_tab');
				$('body').removeClass('mq-lg_tab');
				$('body').removeClass('mq-desktop');
				if(Modernizr.mq('only all and (min-width: 900px)')) {
					$('body').addClass('mq-desktop');
				} else if(Modernizr.mq('only all and (min-width: 700px)')) {
					$('body').addClass('mq-lg_tab');
				} else if(Modernizr.mq('only all and (min-width: 400px)')) {
					$('body').addClass('mq-phone_wide-sm_tab');
				} else {
					$('body').addClass('mq-phone');
				}
			});
		}
	};
})(jQuery, Drupal);

// Rewritten version for correcting a screen-zoom issue on rotation in iOS
// By @mathias, @cheeaun and @jdalton
/*
(function(doc) {

	var addEvent = 'addEventListener',
		type = 'gesturestart',
		qsa = 'querySelectorAll',
		scales = [1, 1],
		meta = qsa in doc ? doc[qsa]('meta[name=viewport]') : [];

	function fix() {
		meta.content = 'width=device-width,minimum-scale=' + scales[0] + ',maximum-scale=' + scales[1];
		doc.removeEventListener(type, fix, true);
	}

	if ((meta = meta[meta.length - 1]) && addEvent in doc) {
		fix();
		scales = [0.25, 1.6];
		doc[addEvent](type, fix, true);
