  let addMyLAUrl = function (url, ltiURLRelative) {
    roles = ENV.current_user_roles;
    if (roles.length == 2 && roles.includes("user") && roles.includes("student")) {
      if (url.includes("files") || url.includes("assignments") || url.includes("grade")) {
        if (url.includes("files")) {
          $(mylalogo +
            '<a href=' + ltiURLRelative + ' class="ef-name-col__link">' +
            '<span class="ef-name-col__text" style="margin: 5px">Resources Accessed</span> </a>').appendTo($('.ef-header__secondary'))
        }
        else if (url.includes("assignments")) {
          $('<div style="display: inline-flex;float: left">'+mylalogo+
            '<a href=' + ltiURLRelative + ' class="ef-name-col__link" style="padding-top:15px">Assignment Planning</a></div>').appendTo($('.header-bar-left'))
        }

        else if (url.includes("grade")) {
          $('<div style="display: inline-flex;float: left">' + mylalogo + '<p><a href=' + ltiURLRelative + ' style="padding: 3px">Grade Distribution</span></a></p></div>')
            .appendTo($("#print-grades-button-container"));
        }
      }
    }
  };
  let enableMyLATool = function () {
    let hostname = window.location.hostname;
    let url = window.location.href
    let mylaToolId = ''
    if (hostname.includes("test")) {
      mylaToolId = '28127'
    } else {
      mylaToolId = '28861'
    }
    if (url.includes("files") || url.includes("assignments") || url.includes("grade")) {
      courseId = url.split('/')[4]
      ltiURL = `/courses/${courseId}/external_tools/${mylaToolId}`
      $("#section-tabs li.section").each(function (i, e) {
        const ltiURLRelative = $(this).children().attr('href');
        if (ltiURLRelative === ltiURL) {
          addMyLAUrl(url, ltiURLRelative)
          return false;
        }
      });
    }

  };

  enableMyLATool();

});



